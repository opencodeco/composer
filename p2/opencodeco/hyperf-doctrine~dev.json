{
    "packages": {
        "opencodeco/hyperf-doctrine": [
            {
                "name": "opencodeco/hyperf-doctrine",
                "version": "dev-main",
                "version_normalized": "dev-main",
                "source": {
                    "type": "git",
                    "url": "https://github.com/opencodeco/hyperf-doctrine.git",
                    "reference": "a8d386a0533156e470443ba3205ddf3cd5cc8791"
                },
                "dist": {
                    "type": "zip",
                    "url": "https://api.github.com/repos/opencodeco/hyperf-doctrine/zipball/a8d386a0533156e470443ba3205ddf3cd5cc8791",
                    "reference": "a8d386a0533156e470443ba3205ddf3cd5cc8791",
                    "shasum": ""
                },
                "require": {
                    "doctrine/orm": "^2.16",
                    "ext-pdo": "*",
                    "hyperf/db": "^3.0",
                    "php": ">=8.1"
                },
                "require-dev": {
                    "friendsofphp/php-cs-fixer": "^3.25",
                    "mockery/mockery": "^1.6",
                    "phpstan/phpstan": "^1.10",
                    "phpunit/phpunit": "^10.3",
                    "spatie/phpunit-watcher": "^1.23",
                    "swoole/ide-helper": "^5.0"
                },
                "time": "2023-09-06T18:14:20+00:00",
                "default-branch": true,
                "type": "library",
                "extra": {
                    "hyperf": {
                        "config": "Hyperf\\Doctrine\\ConfigProvider"
                    }
                },
                "autoload": {
                    "psr-4": {
                        "Hyperf\\Doctrine\\": "src/"
                    }
                },
                "autoload-dev": {
                    "psr-4": {
                        "HyperfTest\\": "tests"
                    }
                },
                "scripts": {
                    "test": [
                        "phpunit --coverage-text"
                    ],
                    "analyse": [
                        "phpstan analyse --memory-limit 128M"
                    ],
                    "cs-fix": [
                        "php-cs-fixer fix $1"
                    ],
                    "ci": [
                        "php-cs-fixer fix --dry-run --diff --verbose",
                        "@composer analyse",
                        "@composer test"
                    ]
                },
                "license": [
                    "MIT"
                ],
                "description": "This project provides an integration for the Doctrine ORM and the Hyperf framework.",
                "keywords": [
                    "doctrine",
                    "hyperf",
                    "orm",
                    "php"
                ],
                "support": {
                    "source": "https://github.com/opencodeco/hyperf-doctrine/tree/main"
                }
            }
        ]
    }
}
